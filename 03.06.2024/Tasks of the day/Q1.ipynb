    "import numpy as np\n",
    "\n",
    "# Create a sample NumPy array\n",
    "arr = np.array([1, 0, 3, 0, -2, 0, 4])\n",
    "\n",
    "# Count the number of non-zero values\n",
    "non_zero_count = np.count_nonzero(arr)\n",
    "\n",
    "print(f\"The number of non-zero values in the array is: {non_zero_count}\")\n",
    "\n"
   ]
  }
